    //let test2 = digitNumber.getResult;\r\n        //console.log(test2);\r\n\r\n        this.clear();\r\n        return;\r\n      }\r\n    });\r\n  }\r\n  disable() {\r\n    this.inputEl.disable = true;\r\n    this.inputEl.placeholder = '게임 끝';\r\n  }\r\n\r\n  clear() {\r\n    this.inputEl.value = '';\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c83d5d05437d5c3f6b7e1bdd4d266593","cacheData":{"env":{}}}